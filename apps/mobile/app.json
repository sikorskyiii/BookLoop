{
  "expo": {
    "name": "BookLoop",
    "slug": "bookloop",
    "scheme": "bookloop",
    "runtimeVersion": { "policy": "sdkVersion" },
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": { "image": "./assets/splash.png", "resizeMode": "contain", "backgroundColor": "#0f1115" },
    "ios": { "supportsTablet": true, "bundleIdentifier": "com.sikorskyiii.bookloop" },
    "android": { "adaptiveIcon": { "foregroundImage": "./assets/icon.png", "backgroundColor": "#0f1115" }, "package": "com.sikorskyiii.bookloop" },
    "web": { "bundler": "metro", "output": "static" }
  }
}
