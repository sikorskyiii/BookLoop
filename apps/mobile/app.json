{
  "expo": {
    "name": "BookLoop",
    "slug": "bookloop",
    
    "scheme": "bookloop",
    "runtimeVersion": {
      "policy": "sdkVersion"
    },
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#0f1115"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.sikorskyiii.bookloop"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#0f1115"
      },
      "package": "com.sikorskyiii.bookloop"
    },
    "web": {
      "bundler": "metro",
      "output": "single"
    },
    "plugins": [
      "expo-secure-store"
    ],
    "extra": {
      "googleIosClientId": "848241555551-244b63p4vciil5s4t5ec9b67tedfs21h.apps.googleusercontent.com",
      "googleAndroidClientId": "848241555551-ag41jq031o4q64qiusbo91e55h58ooma.apps.googleusercontent.com",
      "googleExpoClientId": "848241555551-qo1ca38lr1dk38pet07qam4hf7p108gf.apps.googleusercontent.com",
      "googleWebClientId": "848241555551-qo1ca38lr1dk38pet07qam4hf7p108gf.apps.googleusercontent.com",
      "eas": {
        "projectId": "866668a4-5ea6-40e0-a383-a18394d836c5"
      }
    }
  }
}
